{
	"name": "financial-modeling-prep-data-source",
	"version": "1.0.4",
	"description": "",
	"main": "src",
	"publishConfig": {
		"access": "public"
	},
	"release": {
		"plugins": [
			[
				"@semantic-release/exec",
				{
					"analyzeCommitsCmd": "(git diff-index --quiet ${lastRelease.gitTag} test/__snapshots__/integration.test.js.snap > /dev/null) && echo '' || echo minor"
				}
			],
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			[
				"@semantic-release/npm",
				{
					"npmPublish": true
				}
			],
			[
				"@semantic-release/exec",
				{
					"prepare": "echo 'registry=https://npm.pkg.github.com/skitionek' > .npmrc",
					"publish": "echo 'registry=https://npm.pkg.github.com/skitionek' > .npmrc"
				}
			],
			"@semantic-release/github",
			[
				"@semantic-release/exec",
				{
					"prepare": "rm .npmrc",
					"publish": "rm .npmrc"
				}
			],
			"@semantic-release/changelog",
			"@semantic-release/git"
		],
		"branch": "master"
	},
	"keywords": [
		"financialModelingPrep",
		"stocks",
		"stock market",
		"api",
		"apollo",
		"DataSource"
	],
	"author": {
		"name": "Dominik Maszczyk",
		"email": "Skitionek@gmail.com",
		"web": "https://www.linkedin.com/in/dominik-maszczyk/"
	},
	"maintainers": [
		{
			"name": "Dominik Maszczyk <Skitionek@gmail.com> (https://www.linkedin.com/in/dominik-maszczyk/)"
		}
	],
	"license": "MIT",
	"homepage": "https://github.com/biosustain-gem/financial-modeling-prep-data-source",
	"bugs": {
		"web": "https://github.com/biosustain-gem/financial-modeling-prep-data-source/issues"
	},
	"engines": {
		"node": ">=6.0.0"
	},
	"scripts": {
		"start": "nodemon -e js --exec node --inspect src/index.js",
		"prebuild": "npm run clean",
		"clean": "rm src/index.js & rimraf lib & rimraf mocks || echo 'finished'",
		"test": "jest",
		"test:integration": "npm test -- -i integration",
		"test:cache": "npm test -- -i cache",
		"test:watch": "npm test -- --watch",
		"cover": "jest --coverage",
		"lint": "eslint src test",
		"build": "cross-env BABEL_ENV=production babel src --out-dir ./ --source-maps --ignore \"*.json\""
	},
	"files": [
		"src"
	],
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/preset-env": "^7.9.5",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/git": "^9.0.0",
		"@semantic-release/exec": "^5.0.0",
		"babel-jest": "^25.4.0",
		"cross-env": "^6.0.0",
		"jest": "^24.7.1",
		"jsonic": "^0.3.1",
		"lodash.merge": "^4.6.1",
		"nodemon": "^2.0.0",
		"semantic-release": "^17.0.7",
		"graphql": "^15.0.0"
	},
	"dependencies": {
		"apollo-datasource-rest": "^0.6.1",
		"auto-bind": "^4.0.0"
	},
	"jshintConfig": {
		"node": true,
		"trailingcomma": false,
		"esversion": 6
	},
	"eslintConfig": {
		"extends": [
			"eslint-config-prettier",
			"eslint-config-uber-es2015",
			"prettier"
		],
		"env": {
			"es6": true,
			"node": true,
			"jest/globals": true
		},
		"parser": "babel-eslint",
		"plugins": [
			"babel",
			"prettier",
			"jest"
		],
		"rules": {
			"valid-jsdoc": 0,
			"no-var": 0,
			"max-len": 0,
			"prefer-spread": 1,
			"prefer-template": 1,
			"quote-props": 1,
			"spaced-comment": 1,
			"max-params": 0,
			"no-multiple-empty-lines": 1,
			"no-process-env": 0,
			"no-inline-comments": 0,
			"no-invalid-this": 0,
			"no-unused-expressions": 0,
			"camelcase": 0,
			"consistent-return": 0,
			"comma-dangle": 1,
			"no-magic-numbers": [
				"error",
				{
					"ignore": [
						0,
						1,
						2,
						100
					],
					"enforceConst": true
				}
			],
			"indent": [
				"error",
				"tab"
			],
			"func-names": "off"
		}
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "current"
					}
				}
			]
		],
		"plugins": [
			"@babel/plugin-proposal-class-properties"
		]
	},
	"jest": {
		"moduleFileExtensions": [
			"js"
		],
		"collectCoverageFrom": [
			"src/**/*.{js}"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Skitionek/financial-modeling-prep-data-source.git"
	}
}
